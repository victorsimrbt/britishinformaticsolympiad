    "def adjacent(space):\n",
    "    if space == 0:\n",
    "        adjacent = [1,2,3,4,5,6,7,8]\n",
    "    elif space == 8:\n",
    "        adjacent = [0,7]\n",
    "    else:\n",
    "        adjacent = [space-1,space+1]\n",
    "    return adjacent\n",
    "\n",
    "for i in range(9):\n",
    "    print(i,\":\",adjacent(i))"
   ]
